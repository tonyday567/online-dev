cabal-version: 2.0
name:           online-market
version:        0.2.0.0
synopsis:       online market daily statistics
description:    Applying the online library to daily market returns.
category:       statistics
homepage:       https://github.com/tonyday567/online-dev#readme
bug-reports:    https://github.com/tonyday567/online-dev/issues
author:         Tony Day
maintainer:     tonyday567@gmail.com
copyright:      Tony Day
license:        BSD3
license-file:   LICENSE
build-type:     Simple

source-repository head
  type: git
  location: https://github.com/tonyday567/online-dev

executable online-market-examples
  main-is: examples.hs
  hs-source-dirs:
      ./examples
  ghc-options: -funbox-strict-fields -fforce-recomp -threaded -rtsopts -with-rtsopts=-N
  build-depends:
      ListLike
    , ad
    , base >=4.7 && <5
    , binary
    , bytestring
    , cassava
    , chart-svg
    , foldl
    , generic-lens
    , lens
    , numhask-histogram
    , JuicyPixels
    , numhask-space
    , online
    , optparse-generic
    , reflection
    , tdigest
    , text
    , time
    , vector
  default-language: Haskell2010
